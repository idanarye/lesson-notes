\begin_inset Formula 
\[
x^{\alpha}\cdot\sum_{n=0}^{\infty}a_{n}x^{n}=x^{0}\sum_{n=0}^{\infty}\cfrac{\left(-1\right)^{n}\cdot a_{0}}{\left(2n\right)!}x^{n}
\]

\end_inset

קיבלנו פתרון:
\begin_inset Formula 
\[
y=a_{0}\sum_{n=0}^{\infty}\cfrac{\left(-1\right)^{n}}{\left(2n\right)!}\cdot x^{n}=a_{0}\sum_{n=0}^{\infty}\cfrac{\left(-1\right)^{n}}{\left(2n\right)!}\cdot\sqrt{x}^{2n}\boxed{=a_{0}\cdot\cos\sqrt{x}}
\]

\end_inset

עבור 
\begin_inset Formula $\alpha=\cfrac{1}{2}$
\end_inset

 מתקבל פיתרון נוסף:
\begin_inset Formula 
\[
y_{2}=a_{1}\cdot\sin\sqrt{x}
\]

\end_inset

הפתרון הכללי הוא צ"ל:
\begin_inset Formula 
\[
y=C_{1}y_{1}+C_{2}y_{2}=\boxed{C_{1}\cos\sqrt{x}+C_{2}\sin\sqrt{x}}
\]

\end_inset

הסבר עבור 
\begin_inset Formula $\alpha=\cfrac{1}{2}$
\end_inset

: הרקורסיה
\begin_inset Formula 
\[
a_{n+1}=\cfrac{-a_{n}}{2\left(n+\cfrac{3}{2}\right)\left(2n+2\right)}=\cfrac{-a_{n}}{\left(2n+2\right)\left(2n+3\right)}
\]

\end_inset

מקבלים פתרון נוסף
\begin_inset Formula 
\[
x^{\alpha}\sum_{n=0}^{\infty}a_{n}x^{n}=x^{\frac{1}{2}}\sum_{n=0}^{\infty}\cfrac{\left(-1\right)^{n}\cdot a_{0}}{\left(2n+1\right)!}\cdot x^{n}=a_{0}\sum\cfrac{\left(-1\right)^{n}}{\left(2n+1\right)!}\cdot x^{\frac{1}{2}\left(2n+1\right)}
\]

\end_inset


