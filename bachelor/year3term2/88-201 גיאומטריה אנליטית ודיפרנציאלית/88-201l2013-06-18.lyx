\end_layout

\begin_layout Section*
תרגיל
\end_layout

\begin_layout Standard
למצוא את הקווים הגיאודזיים על הטורוס
\begin_inset Formula 
\[
x\left(\theta,\varphi\right)=\begin{pmatrix}\left(R+r\cos\theta\right)\cos\varphi\\
\left(R+r\cos\theta\right)\sin\varphi\\
r\sin\theta
\end{pmatrix}
\]

\end_inset


\end_layout

\begin_layout Subsection*
פתרון
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\mathrm{d}x=\begin{pmatrix}-r\sin\theta\cos\varphi & -\left(R+r\cos\theta\right)\sin\varphi\\
-r\sin\theta\sin\varphi & \left(R+r\cos\theta\right)\cos\varphi\\
r\cos\theta & 0
\end{pmatrix}
\]

\end_inset


\begin_inset Formula 
\[
G=\begin{pmatrix}r^{2} & 0\\
0 & \left(R+r\cos\theta\right)^{2}
\end{pmatrix}
\]

\end_inset


\begin_inset Formula 
\[
s=\int\sqrt{g_{\theta\theta}\dot{\theta}^{2}+g_{\varphi\varphi}\dot{\varphi}^{2}+2g_{\theta\varphi}\dot{\theta}\dot{\varphi}}\mathrm{\, d}t
\]

\end_inset


\begin_inset Formula 
\[
G^{-1}=\begin{pmatrix}\frac{1}{r^{2}} & 0\\
0 & \frac{1}{\left(R+r\cos\theta\right)^{2}}
\end{pmatrix}
\]

\end_inset


\begin_inset Formula 
\[
\Gamma_{11}^{1}=\frac{1}{2}g^{11}\left(g_{11\prime1}+g_{11\prime1}-g_{11\prime1}\right)+\frac{1}{2}g^{12}\left(g_{12\prime1}+g_{12\prime1}-g_{11\prime2}\right)=0
\]

\end_inset


\begin_inset Formula 
\[
\Gamma_{12}^{1}=\frac{1}{2}g^{11}\left(g_{11\prime2}+g_{12\prime1}-g_{12\prime1}\right)+\frac{1}{2}g^{12}\left(g_{21\prime2}+g_{22\prime1}-g_{12\prime2}=0\right)
\]

\end_inset


\begin_inset Formula 
\[
\Gamma_{22}^{1}=\frac{1}{2}g^{11}\left(g_{21\prime2}+g_{21\prime2}-g_{22\prime1}\right)+\frac{1}{2}g^{12}\left(g_{22\prime2}+g_{22}\cdots\right)=\cdots
\]

\end_inset


\begin_inset Formula 
\[
\cdots=\frac{1}{2}\left(-\frac{1}{r^{2}}-2r\sin\theta\left(R+r\cos\theta\right)\right)=\frac{\sin\theta\left(R+r\cos\theta\right)}{r}
\]

\end_inset


\begin_inset Formula 
\[
\Gamma_{11}^{2}=\frac{1}{2}g^{21}\left(\cdots\right)+\frac{1}{2}g^{22}\left(g_{12\prime1}+g_{12\prime1}-g_{11\prime2}\right)=0
\]

\end_inset


\begin_inset Formula 
\[
\Gamma_{12}^{2}=\frac{1}{2}g^{21}\left(\cdots\right)+\frac{1}{2}g^{22}\left(g_{22\prime1}+g_{21\prime2}-g_{21\prime2}\right)=\frac{r\sin\theta}{R+r\cos\theta}=\Gamma_{21}^{2}
\]

\end_inset


\begin_inset Formula 
\[
\Gamma_{22}^{2}=\frac{1}{2}g^{21}\left(\cdots\right)+\frac{1}{2}g^{22}\left(g_{22\prime2}+g_{22\prime2}-g_{22\prime2}\right)=0
\]

\end_inset


\begin_inset Formula 
\[
\ddot{\gamma}^{i}+\Gamma_{jk}^{i}\dot{\gamma}^{j}\dot{\gamma}^{k}=0
\]

\end_inset


\begin_inset Formula 
\[
0=\ddot{\gamma}^{1}+\xcancel{\Gamma_{11}^{1}\dot{\gamma}^{1}\dot{\gamma}^{1}}+\xcancel{2\Gamma_{12}^{1}\dot{\gamma}^{1}\dot{\gamma}^{2}}+\Gamma_{22}^{1}\dot{\gamma}^{2}\dot{\gamma}^{2}
\]

\end_inset


\begin_inset Formula 
\[
\boxed{0=\ddot{\gamma}^{1}+\frac{\sin\theta\left(R+\cos\theta\right)}{r}\left(\dot{\gamma}^{2}\right)^{2}}
\]

\end_inset


\begin_inset Formula 
\[
0=\ddot{\gamma}^{2}+2\Gamma_{12}^{2}\dot{\gamma}^{1}\dot{\gamma}^{2}
\]

\end_inset


\begin_inset Formula 
\[
\boxed{0=\ddot{\gamma}^{2}+2\frac{r\sin\theta}{R+r\cos\theta}\dot{\gamma}^{1}\dot{\gamma}^{2}}
\]

\end_inset


\end_layout

\begin_layout Standard
מכיוון שהמשוואות הגיאודזיות נותנות עקומות בפרמטריזציה טבעית, אפשר להחליף
 את אחת המשוואות במשוואה
\begin_inset Formula 
\[
g_{11}\left(\dot{\gamma}^{1}\right)^{2}+g_{22}\left(\dot{\gamma}^{2}\right)^{2}=1
\]

\end_inset

ואז
\begin_inset Formula 
\[
r^{2}\left(\dot{\gamma}^{1}\right)^{2}+\left(R+r\cos\theta\right)^{2}\left(\dot{\gamma}^{2}\right)^{2}=1
\]

\end_inset


\begin_inset Formula 
\[
\left(\dot{\gamma}^{2}\right)^{2}=\frac{1-r^{2}\left(\dot{\gamma}^{1}\right)^{2}}{\left(R+r\cos\theta\right)^{2}}
\]

\end_inset


\begin_inset Formula 
\[
0=\ddot{\gamma}^{1}+\frac{\sin\theta\left(R+r\cos\theta\right)}{r}\cdot\frac{1-r^{2}\left(\dot{\gamma}^{1}\right)^{2}}{\left(R+r\cos\theta\right)^{2}}
\]

\end_inset

ואפשר לנסות ולהמשיך לפתור...
\end_layout

\begin_layout Subsection*
פתרון לפי משוואות אויילר לגרנג'
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
s=\int\sqrt{g_{\theta\theta}\dot{\theta}^{2}+g_{\varphi\varphi}\dot{\varphi}^{2}+2g_{\theta\varphi}\dot{\theta}\dot{\varphi}}\mathrm{\, d}t
\]

\end_inset


\begin_inset Formula 
\[
\mathcal{L}=\gamma^{2}\dot{\theta}^{2}+\left(R+r\cos\theta\right)^{2}\dot{\varphi}^{2}
\]

\end_inset


\begin_inset Formula 
\[
\frac{\mathrm{d}}{\mathrm{d}t}\frac{\partial\mathcal{L}}{\partial\dot{\theta}}=\frac{\partial L}{\partial\theta}
\]

\end_inset


\begin_inset Formula 
\[
\frac{\mathrm{d}}{\mathrm{d}t}\left(2r^{2}\dot{\theta}\right)=-2\left(R+r\cos\theta\right)r\sin\theta\dot{\varphi}^{2}
\]

\end_inset


\begin_inset Formula 
\[
\cancel{2}r^{2}\ddot{\theta}=-\cancel{2}\left(R+r\cos\theta\right)r\sin\theta\dot{\varphi}^{2}
\]

\end_inset


