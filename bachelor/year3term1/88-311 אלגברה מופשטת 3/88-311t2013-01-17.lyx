\begin_layout Standard
\begin_inset Formula $\textnormal{Gal}\left(\mathbb{Q}\left(\rho_{n}\right)/\mathbb{Q}\right)=U_{n}$
\end_inset

, ואנחנו רוצים ש
\begin_inset Formula $\left|U_{n}\right|=2^{m}$
\end_inset

, ובחירת 
\begin_inset Formula $n$
\end_inset

 כנ"ל מבטיחה זאת.
\begin_inset Formula 
\[
n=\underbrace{5}_{l=0}\cdot\underbrace{3}_{l=0}
\]

\end_inset

צריך לבנות את 
\begin_inset Formula $\rho_{15}$
\end_inset

:
\begin_inset Formula 
\[
\rho_{15}\neq\rho_{5}\cdot\rho_{3}
\]

\end_inset


\begin_inset Formula 
\[
\rho_{15}=\rho_{5}^{2}+\rho_{3}^{2}
\]

\end_inset


\begin_inset Formula $\rho_{3}$
\end_inset

 ניתן לבנייה: 
\begin_inset Formula $\Phi_{3}\left(x\right)=x^{2}+x+1=0$
\end_inset

 והשורשים הם 
\begin_inset Formula $-\frac{1}{2}\pm\frac{1}{2}\sqrt{3}$
\end_inset

, ולכן 
\begin_inset Formula $\rho_{3}=-\frac{1}{2}+\frac{1}{2}\sqrt{-3}$
\end_inset

.
\end_layout

\begin_layout Standard
לגבי 
\begin_inset Formula $\rho_{5}$
\end_inset

:
\begin_inset Formula 
\[
\xymatrix{E=\mathbb{Q}\left(\rho_{5}\right)\ar@{-}[dd]_{4}\ar@{-}[rd]\\
 & K=\mathbb{Q}\left(\rho_{5}\right)^{\left\langle \sigma^{2}\right\rangle }\ar@{-}[ld]\\
\mathbb{Q}
}
\]

\end_inset


\begin_inset Formula 
\[
\sigma:\rho_{5}\mapsto\rho_{5}^{2}
\]

\end_inset


\begin_inset Formula 
\[
\textnormal{tr}_{E/\mathbb{Q}}\left(\rho_{5}\right)=\rho_{5}+\rho_{5}^{2}+\rho_{5}^{3}+\rho_{5}^{4}=-1
\]

\end_inset


\begin_inset Formula 
\[
\Phi_{5}=x^{4}+x^{3}+x^{2}+x+1
\]

\end_inset


\begin_inset Formula 
\[
\textnormal{tr}_{E/K}\left(\rho_{5}\right)=\rho_{5}+\sigma^{2}\left(\rho_{5}\right)=\rho_{5}+\rho_{5}^{-1}\in K
\]

\end_inset

נמצא פולינום מינימלי מעל 
\begin_inset Formula $\mathbb{Q}$
\end_inset

:
\begin_inset Formula 
\[
\sigma\left(\rho_{5}+\rho_{5}^{-1}\right)=\rho_{5}^{2}+\rho_{5}^{3}
\]

\end_inset


\begin_inset Formula 
\[
\left(x-\left(\rho_{5}+\rho_{5}\right)^{-1}\right)\left(x-\left(\rho_{5}^{2}+\rho_{5}^{3}\right)\right)=x^{2}-\textnormal{tr}_{E/\mathbb{Q}}\left(\rho_{5}\right)x+\left(\rho_{5}+\rho_{5}^{-1}\right)\left(\rho_{5}^{2}+\rho_{5}^{3}\right)=x^{2}+x-1
\]

\end_inset

השורשים הם 
\begin_inset Formula $\frac{-1\pm\sqrt{5}}{2}$
\end_inset

 כלומר 
\begin_inset Formula $\rho_{5}+\rho_{5}^{-1}=\frac{-1+\sqrt{5}}{2}$
\end_inset

.
 נמצא פולינום מינימלי של 
\begin_inset Formula $\rho_{5}$
\end_inset

 מעל 
\begin_inset Formula $K=\mathbb{Q}\left(\sqrt{5}\right)$
\end_inset

: הצמוד של 
\begin_inset Formula $\rho_{5}$
\end_inset

 מעל 
\begin_inset Formula $K$
\end_inset

 הוא 
\begin_inset Formula $\rho_{5}^{-1}=\sigma^{2}\left(\rho_{5}\right)$
\end_inset

.
\begin_inset Formula 
\[
\xymatrix{E\ar@{-}[dd]^{\textnormal{Gal}=\left\langle \sigma^{2}\right\rangle }\\
\\
K
}
\]

\end_inset


\begin_inset Formula 
\[
\left(x-\rho_{5}\right)\left(x-\rho_{5}^{-1}\right)=x^{2}-\left(\rho_{5}+\rho_{5}^{-1}\right)x+1
\]

\end_inset

השורשים הם: 
\begin_inset Formula $\frac{-1+\sqrt{5}}{4}\pm\frac{1}{4}\sqrt{-12-2\sqrt{5}}$
\end_inset

, 
\begin_inset Formula $\rho_{5}=\square+\square$
\end_inset

.
